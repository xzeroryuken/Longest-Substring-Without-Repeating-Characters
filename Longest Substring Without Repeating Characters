class Solution(object):
    def lengthOfLongestSubstring(self, s):
        stringSet = set()
        l = 0 
        res = 0 

        for r in range(len(s)): #
            while s[r] in stringSet:
                stringSet.remove(s[l])
                l += 1
            stringSet.add(s[r])
            res = max(res, r - l + 1)
        return res
